{* Profile Video List CSS styles *}

.video_list {
	padding: 0px;
}

.video_info {
	float: right;
	margin-left: -150px;
	width: 150px;
}

.video_thumb {
	float: left;
	margin-right: -100px;
	width: 100px;
}

.video_body {
	margin-left: 110px;
	margin-right: 155px;
	word-wrap: break-word;
}

.list_item hr {
	border-style: none none solid;
	border-width: medium medium 1px;
	color:#eee;
}

