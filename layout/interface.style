}

.required_star {
	color: red;
}

.photo_details_container .block{
	background: none;
}
.profile_thumb_online {
	display: none;
}
.floatbox_container .fb_content input[type=text],
.floatbox_container .fb_content input[type=password] {
	width: 92%;
}
.floatbox_container .fb_content .block_toolbar {
