.menu-list {
	line-height: 30px;
}
.menu-list a.active,
.menu-list a.active:hover {
	background: none;
	font-weight: normal;
}
.menu-list a {
	padding: 0px 7px;
	font-size: 13px;
	font-weight: bold;
	background: none;
	text-transform: uppercase;
}
.menu-list a,
.menu-list a:hover {
	color: #0799BB;
	text-decoration: none;
	background: none;
}