.photo_block {
	background: #fff;
	border-style: solid;
	border-color: #f9f9f9 #eee #ddd #eee;
	border-width: 1px;
	margin: 0px 8px 9px 0px;
}
.photo_block .block {
	margin: 0px;
	padding: 7px;
}
.inactive_rate_list, .active_rate_list {
	margin-bottom: 4px;
}
.photo_block .small {
	font-size: 10px;
}
.details_cont {
	width: 131px;
}