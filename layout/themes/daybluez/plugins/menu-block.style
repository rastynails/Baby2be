ul.menu-block {
	background: none;
	height: 24px;
	padding: 4px 0px 0px 0px;
}
ul.menu-block li.active {
	height: 22px;
	background: #7a948f;
	border-color: #4b6564 #99b2ae #99b2ae #4c6564;
}
ul.menu-block li.tab {
	border-style: solid;
	border-width: 1px 1px 1px 1px;
	border-color: #aac2bd #4c6565 #4c6565 #aac2bd;
	height: 22px;
	padding: 0px 0px 0px 0px;
	margin: 0px 2px 0px 0px;
	background: #90aaa4;
}
ul.menu-block li.tab:hover {
	background: #7a948f;
	border-color: #99b2ae #4c6564 #4c6564 #99b2ae;
}
ul.menu-block li.tab a {
	height: 22px;
	line-height: 20px;
	padding: 0px 5px;
	color: #fff;
}
ul.menu-block li.active a,
ul.menu-block li.active a.active {
	background-color: transparent;
}
ul.menu-block li.tab a:hover {
	background-color: transparent;
	text-decoration: none;
}
ul.menu-block li.tab a.latest {
	padding-left: 18px;
	background: image(menu_ico_latest.png) no-repeat 2px 50%;
}
ul.menu-block li.tab a.latest:hover {
	background-image: image(menu_ico_latest_act.png);
}
ul.menu-block li.active a.latest {
	background-image: image(menu_ico_latest_act.png);
}
ul.menu-block li.tab a.top_rated,
ul.menu-block li.tab a.popular {
	padding-left: 15px;
	background: image(menu_ico_top.png) no-repeat 3px 50%;
}
ul.menu-block li.tab a.top_rated:hover,
ul.menu-block li.tab a.popular:hover {
	background-image: image(menu_ico_top_act.png);
}
ul.menu-block li.active a.top_rated,
