.event_calendar td.link {
	background-color:#7a948f;
}
.event_calendar td a {
	color: #fff;
	text-decoration: underline;
}
.event_calendar td a:hover {
	color: #fff;
	text-decoration: none;
}
.event_calendar td.current {
	background-color: #5f7978;
	color: #fff;
}
.event_calendar td, .event_calendar th {
	background-color: #8faaa5;
}