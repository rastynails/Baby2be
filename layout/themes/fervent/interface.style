}

$default_color {
	color: << $base_font;
}

$input_bg {
	background: #fbf0f0 image(bg.gif) no-repeat top left;
}

$border {
	border: 1px solid #ab977a;
}

$button {
	background: #de2024 image(button.gif) repeat-x left top;
	border-style: solid;
	border-width: 1px;
	border-color: #a00308;
	color: #fff;
	font-weight: normal;
	display: block;
	border-radius: 4px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	font-family: "Ubuntu", Arial;
	font-weight: lighter;
	overflow: visible;
}
$button_hover {
	border: 1px solid #ec2227;
	text-decoration: none;
}

html body input[type=submit], 
html body input[type=button] {
	padding: 0 14px;
	font-size: 14px;
	height: 28px;
	display: inline;
}

input[type=submit]:hover, 
input[type=button]:hover { 
	background-color: #da1c21 !important;
	border: 1px solid #ec2227 !important;
	background-image: none !important;
	border-top: 1px solid #a00308  !important;
	border-left: 1px solid #a00308  !important;
}

input[type=submit]:disabled,
input[type=button]:disabled {
	background-color: #a00308 !important;
	border: 1px solid #a00308 !important;
	color: #fff !important;
	cursor: default !important;
}

html body input[type=password] {
	height: 22px;
}

$site_logo {
	background: image(logo.png) no-repeat 0px 0px;
}

$page_header {
