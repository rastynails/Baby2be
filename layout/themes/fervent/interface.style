@font-face {
    font-family: 'Ubuntu';
    src: url('/layout/themes/fervent/fonts/ubuntu-regular-webfont.eot');
    src: url('/layout/themes/fervent/fonts/ubuntu-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('/layout/themes/fervent/fonts/ubuntu-regular-webfont.woff') format('woff'),
         url('/layout/themes/fervent/fonts/ubuntu-regular-webfont.ttf') format('truetype'),
         url('/layout/themes/fervent/fonts/ubuntu-regular-webfont.svg#UbuntuBold') format('svg');
    font-weight: normal;
    font-style: normal;
}

$body {
	background: #fff7f1 image(main_bg.jpg) 0 0 repeat;
}

$base_font {
	font-family: Arial;
	font-size: 12px;
	color: #584e4e;
}

$a_link {
	color: #540808;
}

