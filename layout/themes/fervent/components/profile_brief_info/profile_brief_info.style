.profile_activity {
	font-size: 11px;
}
.brief_info_thumb {
	margin-bottom: 0px;
}

div.member_actions a {
	background:#fff7f1 none;
	background-color:#fff7f1;
	border: 1px solid #e6d3bb;
 	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	color: #584e4e;
}
div.member_actions a:hover {
	background-color: #fff7f1;
	border-color: #e6d3bb;
	text-decoration: none;
	color: #df0007;
}