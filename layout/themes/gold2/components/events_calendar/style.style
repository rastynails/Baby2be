.event_calendar td.link {
	background-color:#fdb80e;
}
.event_calendar td a {
	color: #372300;
	text-decoration: underline;
}
.event_calendar td a:hover {
	color: #ae6800;
	text-decoration: none;
}
.event_calendar td.current {
	background: #372300;
	color: #ffce06;
}
.event_calendar td.current a {
	color: #cbb96e;
}
.event_calendar td, .event_calendar th {
	background-color: transparent;
}