.photo_block {
	background: #efc98c;
	border-style: solid;
	border-color: #f8e8cd #f2d4a3 #efca8e #f2d4a3;
	border-width: 1px;
	margin: 0px 8px 9px 0px;
	border-radius: 2px;
 	-moz-border-radius: 2px;
	-webkit-border-radius: 2px;
}
.photo_block .block {
	margin: 0px;
	padding: 7px;
}
.inactive_rate_list, .active_rate_list {
	margin-bottom: 4px;
}
.photo_block .small {
	font-size: 10px;
}
.details_cont {
	width: 130px;
}