@font-face {
    font-family: 'Ubuntu';
    src: url('themes/gold2/fonts/ubuntu-regular-webfont.eot');
    src: url('themes/gold2/fonts/ubuntu-regular-webfont.eot?#iefix') format('embedded-opentype'),
         url('themes/gold2/fonts/ubuntu-regular-webfont.woff') format('woff'),
         url('themes/gold2/fonts/ubuntu-regular-webfont.ttf') format('truetype'),
         url('themes/gold2/fonts/ubuntu-regular-webfont.svg#UbuntuBold') format('svg');
    font-weight: normal;
    font-style: normal;
}

$body {
	background: #fca82e image(inner_bg.jpg) repeat 0 0;
}

$base_font {
	font-family: Arial;
	font-size: 12px;
	color: #5a4424;
}

$a_link {
	color: #372300;
}

