}

$default_color {
	color: << $base_font;
}

$input_bg {
	background: #e4a419 image(bg.png) repeat-x top left;
}

$border {
	border: 1px solid #e5d842;
}

$button {
	background: #f1a800 image(button.gif) repeat-x left top;
	border-style: solid;
	border-width: 1px;
	border-top-color: #fffe75;
	border-bottom-color: #ee9e02;
	border-left-color: #f9bc1b;
	border-right-color: #f9bc1b;
	color: #6e3a00;
	font-weight: normal;
	display: block;
	border-radius: 4px;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	font-family: "Ubuntu", Arial;
	font-weight: lighter;
	overflow: visible;
}
$button_hover {
	border: 1px solid #f9bc1b;
	text-decoration: none;
}

html body input[type=submit], 
html body input[type=button] {
	padding: 0 14px;
	font-size: 14px;
	height: 28px;
	display: inline;
}

input[type=submit]:hover, 
input[type=button]:hover { 
	background-color: #f5c100;
	border-top-color: #fffe75;
	border-bottom-color: #ed9e02;
	border-left-color: #f9b917;
	border-right-color: #f9b917;
	background-image: image(button_hover.gif);
}

input[type=submit]:disabled,
input[type=button]:disabled {
	background-color: #ef9f02 !important;
	border: 1px solid #b26d0c !important;
	color: #6e3a00 !important;
	cursor: default !important;
}

html body input[type=password] {
	height: 22px;
}

