#footer .menu-line-bottom a:hover, #footer .menu-line-bottom a.active
{
	text-decoration:none;
	color: #dff89f;
}

.menu-line-bottom a
{
	text-decoration:underline;
	color: #fff;
	margin-right: 4px;
}

.menu-line-bottom li
{
	list-style:none;
	
	display:inline;
}

ul.menu-line-bottom {
	text-align: left;
	background: none;
	padding: 0px;
	color: #dff89f;
	font-weight: bold;
}