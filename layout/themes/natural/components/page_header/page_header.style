/* -- component page header -- */

.page_header {
	background: none;
	margin: 0 auto 0px;
	height: 52px;
	width: 991px;
	float: none;
	position: relative;
	padding: 25px 0px 0px;
}

.logo {
	width:293px;
	height:52px;
	position:absolute;
	background:image(logo.png) no-repeat left 5px;
	margin: 0px;
	padding: 0px;
	top: 0px;
}