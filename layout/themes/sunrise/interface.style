	color: green;
}


$item {
	border-color: #aaa;
	background: #eee;
}

#footer{
	padding-right: 11px;
	padding-bottom: 11px;
	background: #125da3;
	margin-bottom: -1px;
	width: 819px;
	color: #2b2b2b;
	text-align: center;
}

.forum_search_result .quotetop,
.forum_post_list .quotetop 
{
	background:#4D896D image(css_img_quote.png) no-repeat scroll right center;
	border-color:#9acbb4;
	border-style:dotted dotted none solid;
	border-width:1px 1px 0 4px;
	color:#FFFFFF;
	font-size:10px;
	font-weight:bold;
	margin:8px auto 0;
	padding:3px;
}

.b_control {
	background-color: transparent;
}

.forum_search_result .quotemain,
.forum_post_list .quotemain 
{
	background:#eee none repeat scroll 0 0;
	border-color: #457A62;
	border-style:none dotted dotted solid;
	border-width:0 1px 1px 4px;
	color:#2b2b2b;
	margin:0 auto 8px;
	padding:4px;
}

.chat_input_container #httpdoc-send_btn{
