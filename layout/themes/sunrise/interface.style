	color: #F4F5D5;
}

.block {
	border: none;
	margin: 0px 0px 14px 0px;
}

.block_cap {
	background:image(block_cap.jpg) repeat;
	border-bottom: 1px dashed #363636;
	border-top: none;
	height: 22px;
}

