}

$hover_location {
	background-color: #777;
}

$button {
	background: image(button.gif) repeat-x;
	border: 1px solid #545454;
	color: #2b2b2b;
}

$button_hover {
	border: 1px solid #fff;
}

$form_th{
	background: #ddd;
}

.tooltip table {
	background: #888;
}

.field_mileage .tab_container .active {
	background-color: #fff;
	border-bottom: none;
}

.field_mileage .country_list .country {
	background-color: #ccc;
}

$border {
	background-color: #fff;
}

$tab {
	border-bottom: none;
	background: #aaa;
}

