	border: none;
	background: #fdbb69;
}

.video_list .highlight,
.block_body .highlight,
.list_stat .highlight {
	color: #AB730B;
} 

$item {
	border-color: #AB730B;
}

.forum_search_result .quotetop,
.forum_post_list .quotetop {
	background:#ffe7b0 image(css_img_quote.gif) no-repeat scroll right center;
	border-color:#FED599;
	border-style:dotted dotted none solid;
	border-width:1px 1px 0 4px;
	color:##5C3306;
	font-size:10px;
	font-weight:bold;
	margin:8px auto 0;
	padding:3px;
}

.forum_search_result .quotemain,
.forum_post_list .quotemain {
	background:#fff2d6 none repeat scroll 0 0;
	border-color: #FED599;
	border-style:none dotted dotted solid;
	border-width:0 1px 1px 4px;
	color:##5C3306;
	margin:0 auto 8px;
	padding:4px;
}

