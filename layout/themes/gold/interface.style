	background:#fff;
	height:18px;
	padding:3px 7px 0 0;
	border-right:1px solid #D79D0F;
	border-left:1px solid #D79D0F;
}

*+html .quick_search hr {
	 display: none;
}

div.wide {
	width: 64%;
}

div.wider {
	width: 66%;
}

div.narrow {
	width: 35%; 
	padding-wright: 8px;
}

#content .video_view .narrow {
	width: 35%; 
	/*padding-right: 48px;*/
}

div.narrower {
	width: 33%;
	padding-right: 1px;
}

