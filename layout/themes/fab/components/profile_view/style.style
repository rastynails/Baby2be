.delete_cmp,.edit_cmp {
	margin: 0px 0px 0px 1px;
}
.search_btns_wrap {
	float: right;
	font-size: 10px;
	color: #B9DDCD;
	margin: 0px 0px 0px;
}
.search_btns_wrap .profile_carousel {
	margin: 0px;
	padding: 0px 0px 10px;
}
.search_btns_wrap a {
	font-size: 11px;
}
.search_btns a:hover {
	text-decoration: none;
}
.search_btns .profile_carousel_prev {
	padding-right: 2px;
}
.search_btns .profile_carousel_next {
	padding-left: 2px;
}
.search_btns .profile_carousel_back {
	padding: 0px 2px;
}
.report_btn {
	float: right;
	padding-left: 8px;
}
.profile_details .menu {
	height: 25px;
	padding-bottom: 3px;
}
.profile_details .menu ul.menu-block {
	padding: 0px;
}
.member_actions {
	padding: 0px 0px 0px 96px;
}
div.member_actions a {
	display: inline-block;
	border-style: solid;
	border-width: 1px 1px 1px 1px;
	border-color: #60758b #28384a #28384a #60758b;
	line-height: 21px;
	height: 22px;
	padding: 0px 8px 0px 8px;
	margin: 0px 2px 0px 0px;
	background: #3e5368;
	color: #fff;
}
div.member_actions a:hover {
	text-decoration: none;
	background: #4d6176;
	border-color: #2c3d4e #445766 #425463 #2c3d4e;
}
.member_actions div {
    float: left;
}
.search_btns .profile_carousel {
	width: 300px;
}