.event_calendar td.link {
	background-color:#486277;
}
.event_calendar td a {
	color: #fff;
	text-decoration: underline;
}
.event_calendar td a:hover {
	color: #fff;
	text-decoration: none;
}
.event_calendar td.current {
	background-color: #162B3B;
	color: #fff;
}
.event_calendar td, .event_calendar th {
	background-color: #667D93;
}