.photo_list {
	padding-left: 10px;
}
.photo_list .highlight {
	color: #8B2053;
}
.photo_list .photo_block {
	background: #fcf7f9;
	padding-top: 12px;
	padding-left: 3px;
	margin-right: 5px;
	height: 120px;
	overflow: hidden;
}
.photo_list .photo_block .block {
	margin: 0px;
}
.photo_list .photo_block .small {
	font-size: 10px;
}
.photo_list .photo_block .details_cont {
	width: 147px;
}
.photo_list .photo_block .details_cont .inactive_rate_list {
	padding-bottom: 5px;
}