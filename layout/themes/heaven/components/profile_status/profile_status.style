.profile_status_toggle {
	float: right;
	line-height: 25px;
	background: image(menu_block.png) no-repeat right 0px;
}
.profile_status_toggle a {
	display: block;
	background: image(menu_block_tail.gif) no-repeat left 0px;
	padding: 0px 6px;
}
.profile_status td {
	padding-top: 4px;
	padding-bottom: 4px;
}
.profile_status td .bold {
	color: #40688f;
}