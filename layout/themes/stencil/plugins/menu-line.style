ul.menu-line {
	padding: 0px;
	color: #006FBD;
	font-size: 12px;
	font-family: Arial;
}
ul.menu-line li {
	padding-left: 4px;
}
ul.menu-line a {
	color: #006FBD;
	font-weight: bold;
}
ul.menu-line a:hover {
	color:#fff;
}
#page_canvas ul.menu-line li a.active {
	color: #fff;
	text-decoration: none;
}