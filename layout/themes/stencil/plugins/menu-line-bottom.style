ul.menu-line-bottom {
	background: #000;
	border-bottom: 1px solid #2a2a29;
	text-align: left;
	padding: 10px 30px;
	color: #a2a2a2;
	margin: 0px;
}
ul.menu-line-bottom li {
	padding-right: 0px;
}
ul.menu-line-bottom li a {
	color: #a2a2a2;
	font-size: 13px;
	padding-right: 2px;
}
#footer ul.menu-line-bottom li a.active {
	text-decoration: none;
}